dir_workers = 5
file_workers = 5

elastic_url = "http://localhost:9200"
debug = true

[rabbitmq]
connect_string = "amqp://guest:guest@localhost:5672/"

[datasource]

[datasource.home]
type = "posix"
path = "/Users/username/tmp"
mount = false
write = true
skip_files_newer_minutes=60
skip_files_older_minutes=0

[datasource.panda]
type = "lustre"
path = "/mnt/panda"
mount = true
write = false
skip_files_newer_minutes=0
skip_files_older_minutes=0
